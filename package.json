{
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "esbuild ./src/app.ts --bundle --outfile=dist/app.js --platform=node",
        "compile": "tsc",
        "lint": "eslint --fix src",
        "local": "yarn build && node dist/app.js",
        "make": "yarn update-api-sdk && yarn lint && yarn compile && yarn build",
        "update-api-sdk": "openapi-generator-cli generate"
    },
    "dependencies": {
        "axios": "^1.7.7",
        "dotenv": "^16.4.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@openapitools/openapi-generator-cli": "^2.13.9",
        "@types/node": "^22.5.5",
        "@typescript-eslint/eslint-plugin": "^8.6.0",
        "@typescript-eslint/parser": "^8.6.0",
        "esbuild": "^0.24.0",
        "eslint": "^9.11.1",
        "globals": "^15.9.0",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "typescript-eslint": "^8.7.0",
        "typescript": "^5.6.0"
    }
}
