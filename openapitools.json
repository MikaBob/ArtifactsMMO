{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 4,
    "generator-cli": {
        "version": "7.8.0",
        "generators": {
            "v1.0": {
                "generatorName": "typescript-axios",
                "output": "packages/artifactsmmo-sdk",
                "inputSpec": "https://api.artifactsmmo.com/openapi.json",
                "additionalProperties": {
                    "apiPackage": "api",
                    "modelPackage": "models",
                    "npmName": "artifactsmmo-sdk",
                    "npmVersion": "2.0.0",
                    "supportsES6": "true",
                    "withSeparateModelsAndApi": true
                }
            }
        }
    }
}
